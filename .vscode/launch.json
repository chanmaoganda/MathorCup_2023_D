{
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "debug runner",
            "cargo": {
                "args": [
                    "build",
                    "--bin=runner",
                ]
            },
            "args": []
        },
        {
            "name": "run.py",
            "type": "debugpy",
            "request": "launch",
            "program": "./crates/annealers/run.py",
            "console": "integratedTerminal"
        },
        {
            "name": "TestDebugging",
            "type": "debugpy",
            "request": "launch",
            "program": "./crates/annealers/test.py",
            "console": "integratedTerminal",
            "args": [
                "1"
            ]
        },
        {
            "name": "MathorCupQ2",
            "type": "debugpy",
            "request": "launch",
            "program": "MathorCupQ2.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
